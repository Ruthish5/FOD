import numpy as np
fuel_efficiency = np.array([25, 30, 35, 40, 45])
average_efficiency = np.mean(fuel_efficiency)
efficiency_model_1 = fuel_efficiency[1]
efficiency_model_2 = fuel_efficiency[4]
percentage_improvement = ((efficiency_model_2 - efficiency_model_1) / efficiency_model_1) * 100
print("Average fuel efficiency:", average_efficiency)
print("Percentage improvement from model 1 to model 2:", percentage_improvement, "%")
